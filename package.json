{
  "name": "bot-sro-mobile-gui",
  "version": "1.0.0",
  "description": "Interface gr√°fica para Bot SRO Mobile",
  "main": "electron/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder",
    "dist": "electron-builder --publish never"
  },
  "keywords": [
    "bot",
    "sro",
    "mobile",
    "adb"
  ],
  "author": {
    "name": "joaodematejr",
    "email": "joaodematejr@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "build": {
    "appId": "com.joaodematejr.botsromobile",
    "productName": "Bot SRO Mobile",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!venv_labelimg/**",
      "!node_modules/**/.cache/**"
    ],
    "extraResources": [
      "simple_bot.py",
      "README.md"
    ],
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Utility",
      "maintainer": "joaodematejr <you@example.com>"
    },
    "win": {
      "target": ["nsis"],
      "artifactName": "${productName}-Setup-${version}.${ext}"
    },
    "mac": {
      "target": ["dmg"],
      "artifactName": "${productName}-${version}.${ext}"
    }
  }
}
